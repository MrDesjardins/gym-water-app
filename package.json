{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "npm run copy:env && vite",
    "dev": "npm run copy:env && vite",
    "build": "vite build",
    "serve": "vite preview",
    "prettier": "prettier --write src/**/*.{ts,tsx,css}",
    "start:server": "npm run copy:env && cd server && ts-node-dev ./server.ts",
    "start:server:debug": "npm run copy:env && cd server && ts-node-dev  --project ./server/tsconfig.json --inspect --respawn ./server/server.ts",
    "copy:env": "copyfiles .env server && copyfiles .env src "
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ws": "^8.5.3",
    "copyfiles": "^2.4.1",
    "prettier": "2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "@solid-primitives/debounce": "^1.1.2",
    "@solid-primitives/throttle": "^1.1.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "solid-app-router": "^0.3.0",
    "solid-icons": "^0.3.3",
    "solid-js": "^1.3.7",
    "ts-node-dev": "^1.1.8",
    "ws": "^8.5.0"
  }
}
